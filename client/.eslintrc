{
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "eslint:recommended"
    ],
    "env": {
      "browser": true
    },
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ],
    "rules": {
      "react/prefer-stateless-function": 0,
      "react/jsx-filename-extension": 0,
      "linebreak-style": 0,
      "react/forbid-prop-types": 0
    }
  }